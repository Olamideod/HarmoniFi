{
    "version": 2,
    "builds": [
        {
        "src": "spotify_clone/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdasize": "15mb", "runtime": "python3.9"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "spotify_clone/wsgi.py"
        }
    ]
}